{
  "expo": {
    "name": "ridesafe",
    "slug": "ridesafe",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.ridesafe"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#000000"
      },
      "package": "com.anonymous.ridesafe",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "YOUR_GOOGLE_MAPS_API_KEY"
        }
      }
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "updates": {
      "enabled": false,
      "checkAutomatically": "NEVER",
      "fallbackToCacheTimeout": 0
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ridesafe"
      },
      "offlineMode": true,
      "expoClient": {
        "useNoDevForceEnable": true,
        "telemetryDisabled": true,
        "experimentalDisabledFastRefresh": false
      }
    },
    "runtimeVersion": "1.0.0",
    "scheme": "ridesafe",
    "newArchEnabled": true
  }
}
